{% extends '::base.html.twig'%}
{% block title %}Editar Fabricante{% endblock %}
{% block head %}{% endblock %}
{% block body %}
	<div class="container">
		<h1>Editar Fabricante</h1>
		<form action="{{ path('fabricante_update', {'id':entity.id}) }}" method="post">
			{{ form_widget(form) }}
			<div class="form-actions">
				<button class="btn btn-primary">Salvar</button>
				<a class="btn btn-success" href="{{ path('fabricantes_lista') }}">Voltar</a>
			</div>
		</form>
		
	</div>	
{% endblock %}